{
  "name": "house-base",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "local": "cross-env APP_ENV=local webpack-dev-server --config=webpack.config.local.js",
    "build": "webpack --config=webpack.config.pro.js",
    "mock": "nodemon ./mock-server.js",
    "start": "npm run mock && npm run local",
    "dev": "cross-env APP_ENV=dev npm run build",
    "test": "cross-env APP_ENV=trunk webpack --config=webpack.config.pro.js",
    "test_trunk": "cross-env APP_ENV=trunk webpack --config=webpack.config.pro.js",
    "pre": "cross-env APP_ENV=huidu webpack --config=webpack.config.pro.js",
    "pro": "cross-env APP_ENV=online webpack --config=webpack.config.pro.js",
    "husky": "npx husky install && npx husky add .husky/pre-commit \"npm run lint-staged\"",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx --format=pretty \"./src\" ",
    "lint:fix": "npm run lint:fix:js && npm run lint:style",
    "lint-staged": "lint-staged",
    "lint:fix:js": "eslint --fix --ext .js,.jsx,.ts,.tsx --format=pretty \"./src\" ",
    "lint:style": "stylelint --fix \"src/**/*.scss\" --syntax scss",
    "lint-staged:js": "eslint --fix --ext .js,.jsx,.ts,.tsx",
    "lint-staged:style": "stylelint --fix --syntax scss"
  },
